<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title><%= title %></title>
<link rel="stylesheet" href="/css/main.css">

<!-- Dynamically injected styles -->
<% if (locals.styles) { %>
    <% styles.forEach(style => { %>
        <%- style %>
    <% }); %>
<% } %>

<!-- Dynamically injected scripts -->
<% if (locals.scripts) { %>
    <% scripts.forEach(script => { %>
        <%- script %>
    <% }); %>
<% } %>